<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sheet1>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Title</sheetTitle>
                    <sheetDescription>Descr.</sheetDescription>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.distance>
						<TCEforms>
							<label>Abstand nach Oben</label>
							<onChange>reload</onChange>
							<config>
								<type>select</type>
								<renderType>selectSingle</renderType>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">kein Abstand</numIndex>
										<numIndex index="1">nodistance</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">normaler Abstand</numIndex>
										<numIndex index="1">distance</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</settings.distance>
                    <settings.headline>
                        <TCEforms>
                            <label>Überschrift</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                                <size>30</size>
                            </config>
                        </TCEforms>
                    </settings.headline>
                    <settings.text>
                        <TCEforms>
                            <label>Unterüberschrift</label>
                            <config>
                                <type>text</type>
                                <rows>5</rows>
                                <cols>30</cols>
                                <eval>trim</eval>
                                <max>100</max>
                                <enableRichtext>1</enableRichtext>
                                <richtextConfiguration>default</richtextConfiguration>
                            </config>
                        </TCEforms>
                    </settings.text>
                    <settings.img>
						<TCEforms>
							<label>Bild</label>
							<config>										
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>gif, jpg,jpeg,png,csv</allowed>
								<max_size>10000</max_size>
								<uploadfolder>uploads/tx_fsfzolleraccordion</uploadfolder>
								<maxitems>1</maxitems>
								<size>1</size>
								<selectedliststyle>Width:180px</selectedliststyle>
							</config>
						</TCEforms>
					</settings.img>
                    <settings.video>
						<TCEforms>
							<label>Video</label>
							<config>
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>mp4</allowed>
								<max_size>10000</max_size>
								<uploadfolder>uploads/tx_fsfzolleraccordion</uploadfolder>
								<maxitems>1</maxitems>
								<size>1</size>
								<selectedliststyle>Width:180px</selectedliststyle>
							</config>
						</TCEforms>
					</settings.video>
                    <settings.references>
                        <section>1</section>
                        <type>array</type>
                        <el>
                            <item>
                                <type>array</type>
                                <title></title>
                                <el>
                                    <headline>
                                        <TCEforms>
                                            <label>Überschrift</label>
                                            <config>
                                                <type>input</type>
                                                <eval>trim</eval>
                                                <size>30</size>
                                            </config>
                                        </TCEforms>
                                    </headline>
                                    <text>
                                        <TCEforms>
                                            <label>Inhaltstext</label>
                                            <config>
                                                <type>text</type>
                                                <rows>5</rows>
                                                <cols>30</cols>
                                                <eval>trim</eval>
                                                <enableRichtext>1</enableRichtext>
                                                <richtextConfiguration>default</richtextConfiguration>
                                            </config>
                                        </TCEforms>
                                    </text>
                                    <img>
                                        <TCEforms>
                                            <label>Bild</label>
                                            <config>										
                                                <type>group</type>
                                                <internal_type>file</internal_type>
                                                <allowed>gif, jpg,jpeg,png,csv</allowed>
                                                <max_size>10000</max_size>
                                                <uploadfolder>uploads/tx_fsfzolleraccordion</uploadfolder>
                                                <maxitems>1</maxitems>
                                                <size>1</size>
                                                <selectedliststyle>Width:180px</selectedliststyle>
                                            </config>
                                        </TCEforms>
                                    </img>
                                    <video>
                                        <TCEforms>
                                            <label>Video</label>
                                            <config>
                                                <type>group</type>
                                                <internal_type>file</internal_type>
                                                <allowed>mp4, mov</allowed>
                                                <max_size>10000</max_size>
                                                <uploadfolder>uploads/tx_fsfzolleraccordion</uploadfolder>
                                                <maxitems>1</maxitems>
                                                <size>1</size>
                                                <selectedliststyle>Width:180px</selectedliststyle>
                                            </config>
                                        </TCEforms>
                                    </video>
                                </el>
                            </item>
                        </el>
                    </settings.references>
                </el>
            </ROOT>
        </sheet1>
    </sheets>
</T3DataStructure>
